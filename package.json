{
  "name": "@xianjianlf2/vue-pdf-view",
  "type": "module",
  "version": "1.0.9",
  "description": "A vue3 component for pdf viewer",
  "author": "xianjianlf2",
  "repository": {
    "type": "git",
    "url": "https://github.com/xianjianlf2/vue3-pdf-viewer.git"
  },
  "keywords": [
    "vue3",
    "pdf viewer",
    "pdfjs"
  ],
  "exports": {
    ".": {
      "require": "./dist/pdf-viewer.umd.js",
      "import": "./dist/pdf-viewer.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/pdf-viewer.umd.cjs",
  "module": "./dist/pdf-viewer.js",
  "types": "./dist/components/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "pdfjs-dist": "^3.8.162",
    "vue": "^3.3.4",
    "vue-virtual-scroller": "2.0.0-beta.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.26",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^3.3.1",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
